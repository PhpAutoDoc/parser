{
  "name": "phpautodoc/parser",
  "require": {
    "php": ">=7.3.0",
    "setbased/error-handler": "^1.0",
    "setbased/php-stratum": "^6.0",
    "setbased/php-stratum-common": "^5.0",
    "setbased/php-stratum-sqlite-pdo": "^1.0",
    "symfony/console": "^5.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^7.0 || ^8.0",
    "plaisio/event-core": "^1.0",
    "setbased/phing-extensions": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "PhpAutoDoc\\Parser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpAutoDoc\\Parser\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
