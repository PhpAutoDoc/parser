{
  "name": "phpautodoc/parser",
  "require": {
    "php": ">=7.3.0",
    "ext-json": "*",
    "setbased/error-handler": "^1.2.0",
    "setbased/php-stratum": "^6.1.0",
    "setbased/php-stratum-common": "^6.2.0",
    "setbased/php-stratum-sqlite-pdo": "^1.5.0",
    "symfony/console": "^5.2.0"
  },
  "require-dev": {
    "phing/phing": "^2.16.3",
    "phpunit/phpunit": "9.5.0",
    "plaisio/event-core": "^2.0.0",
    "setbased/phing-extensions": "^2.1.0",
    "squizlabs/php_codesniffer": "^3.5.8"
  },
  "autoload": {
    "psr-4": {
      "PhpAutoDoc\\Parser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpAutoDoc\\Parser\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
